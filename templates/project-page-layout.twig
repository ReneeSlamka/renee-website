{% extends 'layout.twig' %}


{% block content %}
    <div class="project-background-container">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <header>
                        {% block projectTitle %}
                        {% endblock %}
                    </header>
                    <p>
                        {% block projectDescription %}
                        {% endblock %}
                    </p>
                </div>
                <div class="col-md-6">
                    {% block projectImage %}
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}